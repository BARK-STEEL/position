<header id="header" class="row">
  <h2 id="logo" class="col-xs-8">Position</h2>
</header>
<div class="container">
<h2>Please Log In</h2>

<%= form_tag '/sessions', method: :post do %>

  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag 'Log In' %>

<% end %>

<%= link_to 'Need to sign up? Register here', register_path, method: :get %>
</div>
