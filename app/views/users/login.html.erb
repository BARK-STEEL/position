<h2>Please Log In</h2>

<%= form_tag '/sessions', method: :post do %>

  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag 'Log In' %>

<% end %>

<%= link_to 'Need to sign up? Register here', register_path, method: :get %>
